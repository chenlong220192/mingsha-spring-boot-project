{
  "groups": [
    {
      "name": "logging",
      "type": "site.mingsha.boot.starter.logging.autoconfigure.MingshaLoggingProperties",
      "sourceType": "site.mingsha.boot.starter.logging.autoconfigure.MingshaLoggingProperties"
    }
  ],
  "properties": [
    {
      "name": "logging.interceptor-enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用 Mingsha Logging 拦截器",
      "defaultValue": true
    },
    {
      "name": "logging.interceptor.include-headers",
      "type": "java.lang.Boolean",
      "description": "是否包含请求头",
      "defaultValue": true
    },
    {
      "name": "logging.interceptor.include-body",
      "type": "java.lang.Boolean",
      "description": "是否包含请求体",
      "defaultValue": false
    },
    {
      "name": "logging.interceptor.max-body-length",
      "type": "java.lang.Integer",
      "description": "请求体最大长度",
      "defaultValue": 1024
    }
  ]
} 