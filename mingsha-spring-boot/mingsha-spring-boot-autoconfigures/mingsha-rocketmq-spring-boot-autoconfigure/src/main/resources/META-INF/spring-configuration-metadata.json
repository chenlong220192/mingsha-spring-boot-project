{
  "groups": [
    {
      "name": "rocketmq",
      "type": "site.mingsha.boot.starter.rocketmq.autoconfigure.MingshaRocketMQProperties",
      "sourceType": "site.mingsha.boot.starter.rocketmq.autoconfigure.MingshaRocketMQProperties"
    }
  ],
  "properties": [
    {
      "name": "rocketmq.enabled",
      "type": "java.lang.Boolean",
      "description": "是否启用 RocketMQ",
      "defaultValue": true
    },
    {
      "name": "rocketmq.producer-group",
      "type": "java.lang.String",
      "description": "生产者组名",
      "defaultValue": "mingsha-producer-group"
    },
    {
      "name": "rocketmq.consumer-group",
      "type": "java.lang.String",
      "description": "消费者组名",
      "defaultValue": "mingsha-consumer-group"
    },
    {
      "name": "rocketmq.send-message-timeout",
      "type": "java.lang.Integer",
      "description": "消息发送超时时间（毫秒）",
      "defaultValue": 3000
    },
    {
      "name": "rocketmq.compress-message-body-threshold",
      "type": "java.lang.Integer",
      "description": "消息压缩阈值（字节）",
      "defaultValue": 4096
    },
    {
      "name": "rocketmq.max-message-size",
      "type": "java.lang.Integer",
      "description": "最大消息大小（字节）",
      "defaultValue": 4194304
    },
    {
      "name": "rocketmq.retry-times-when-send-failed",
      "type": "java.lang.Integer",
      "description": "重试次数",
      "defaultValue": 2
    },
    {
      "name": "rocketmq.retry-times-when-send-async-failed",
      "type": "java.lang.Integer",
      "description": "异步发送重试次数",
      "defaultValue": 2
    },
    {
      "name": "rocketmq.enable-msg-trace",
      "type": "java.lang.Boolean",
      "description": "是否启用消息轨迹",
      "defaultValue": false
    },
    {
      "name": "rocketmq.customized-trace-topic",
      "type": "java.lang.String",
      "description": "消息轨迹主题",
      "defaultValue": "RMQ_SYS_TRACE_TOPIC"
    }
  ]
} 