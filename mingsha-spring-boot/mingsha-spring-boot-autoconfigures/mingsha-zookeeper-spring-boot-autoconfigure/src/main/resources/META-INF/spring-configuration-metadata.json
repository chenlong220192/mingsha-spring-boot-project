{
  "groups": [
    {
      "name": "spring.zookeeper",
      "type": "site.mingsha.boot.starter.zookeeper.autoconfigure.MingshaZookeeperProperties",
      "sourceType": "site.mingsha.boot.starter.zookeeper.autoconfigure.MingshaZookeeperProperties"
    }
  ],
  "properties": [
    {
      "name": "spring.zookeeper.connect-string",
      "type": "java.lang.String",
      "description": "Zookeeper服务器地址",
      "defaultValue": "localhost:2181"
    },
    {
      "name": "spring.zookeeper.session-timeout-ms",
      "type": "java.lang.Integer",
      "description": "会话超时时间（毫秒）",
      "defaultValue": 60000
    },
    {
      "name": "spring.zookeeper.connection-timeout-ms",
      "type": "java.lang.Integer",
      "description": "连接超时时间（毫秒）",
      "defaultValue": 15000
    },
    {
      "name": "spring.zookeeper.retry-policy",
      "type": "java.lang.String",
      "description": "重试策略类型",
      "defaultValue": "exponential"
    },
    {
      "name": "spring.zookeeper.retry-count",
      "type": "java.lang.Integer",
      "description": "重试次数",
      "defaultValue": 3
    },
    {
      "name": "spring.zookeeper.retry-interval-ms",
      "type": "java.lang.Integer",
      "description": "重试间隔（毫秒）",
      "defaultValue": 1000
    },
    {
      "name": "spring.zookeeper.max-retry-interval-ms",
      "type": "java.lang.Integer",
      "description": "最大重试间隔（毫秒）",
      "defaultValue": 30000
    },
    {
      "name": "spring.zookeeper.namespace",
      "type": "java.lang.String",
      "description": "命名空间",
      "defaultValue": ""
    }
  ]
} 