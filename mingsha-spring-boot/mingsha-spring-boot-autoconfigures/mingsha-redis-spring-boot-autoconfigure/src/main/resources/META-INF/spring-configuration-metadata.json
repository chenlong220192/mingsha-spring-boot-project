{
  "groups": [
    {
      "name": "spring.redis",
      "type": "site.mingsha.boot.starter.redis.autoconfigure.MingshaRedisProperties",
      "sourceType": "site.mingsha.boot.starter.redis.autoconfigure.MingshaRedisProperties"
    }
  ],
  "properties": [
    {
      "name": "spring.redis.host",
      "type": "java.lang.String",
      "description": "Redis服务器地址",
      "defaultValue": "localhost"
    },
    {
      "name": "spring.redis.port",
      "type": "java.lang.Integer",
      "description": "Redis服务器端口",
      "defaultValue": 6379
    },
    {
      "name": "spring.redis.password",
      "type": "java.lang.String",
      "description": "Redis密码"
    },
    {
      "name": "spring.redis.database",
      "type": "java.lang.Integer",
      "description": "数据库索引",
      "defaultValue": 0
    },
    {
      "name": "spring.redis.timeout",
      "type": "java.lang.Integer",
      "description": "连接超时时间（毫秒）",
      "defaultValue": 2000
    },
    {
      "name": "spring.redis.max-active",
      "type": "java.lang.Integer",
      "description": "连接池最大连接数",
      "defaultValue": 8
    },
    {
      "name": "spring.redis.max-idle",
      "type": "java.lang.Integer",
      "description": "连接池最大空闲连接数",
      "defaultValue": 8
    },
    {
      "name": "spring.redis.min-idle",
      "type": "java.lang.Integer",
      "description": "连接池最小空闲连接数",
      "defaultValue": 0
    },
    {
      "name": "spring.redis.max-wait",
      "type": "java.lang.Long",
      "description": "连接池最大等待时间（毫秒）",
      "defaultValue": -1
    }
  ]
} 